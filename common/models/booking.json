      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "vehicle": {
      "type": "belongsTo",
      "model": "Vehicle",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "route": {
